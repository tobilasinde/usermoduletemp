    "cookie-parser": "^1.4.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "cross-env": "^3.2.4",
    "ejs": "^3.1.3",
    "expect.js": "^0.3.1",
