{
  "name": "starter",
  "version": "1.0.0",
  "description": "Starter kit for User Module",
  "private": true,
  "scripts": {
    "start": "DEBUG=express-sequelize node ./bin/www",
    "start-dev": "cross-env DEBUG=express-sequelize nodemon ./bin/www",
